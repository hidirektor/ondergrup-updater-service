<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>ondergrup-updater-service.jar</jar>
  <outfile>windows_Updater.exe</outfile>
  <errTitle>Programın çalışabilmesi için JRE 1.8.0 ve JDK 12.0.2 Gereklidir !</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>high</priority>
  <downloadUrl>https://www.oracle.com/tr/java/technologies/javase/jdk12-archive-downloads.html</downloadUrl>
  <supportUrl>https://github.com/hidirektor</supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>true</restartOnCrash>
  <manifest>app.manifest</manifest>
  <icon>src\main\resources\assets\images\logo.ico</icon>
  <singleInstance>
    <mutexName>ondergrup_updater</mutexName>
    <windowTitle>Önder Updater</windowTitle>
  </singleInstance>
  <jre>
    <path>%JAVA_HOME%;%PATH%</path>
    <requiresJdk>true</requiresJdk>
    <requires64Bit>false</requires64Bit>
    <minVersion>1.8.0</minVersion>
    <maxVersion></maxVersion>
    <opt>-Dfile.encoding=&quot;UTF-8&quot;</opt>
  </jre>
  <versionInfo>
    <fileVersion>1.0.0.0</fileVersion>
    <txtFileVersion>1.0.0.0</txtFileVersion>
    <fileDescription>Önder Updater</fileDescription>
    <copyright>Halil İbrahim Direktör</copyright>
    <productVersion>1.0.0.0</productVersion>
    <txtProductVersion>1.0.0.0</txtProductVersion>
    <productName>Önder Updater</productName>
    <companyName>Önder Grup</companyName>
    <internalName>Launcher</internalName>
    <originalFilename>windows_Updater.exe</originalFilename>
    <trademarks></trademarks>
    <language>TURKISH</language>
  </versionInfo>
</launch4jConfig>